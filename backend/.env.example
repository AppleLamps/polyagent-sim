# Required: xAI API Key (get from https://x.ai)
XAI_API_KEY=your_xai_api_key_here

# Optional: AI model for analysis (default: grok-4-1-fast)
# Options: grok-4-1-fast, grok-beta, etc.
XAI_MODEL=grok-4-1-fast

# Optional: Database URL (default: sqlite:///./polyagent.db)
DATABASE_URL=sqlite:///./polyagent.db

# Optional: Initial portfolio balance in USDC (default: 100000)
INITIAL_BALANCE=100000

# Optional: Rate limiting settings
RATE_LIMIT_ANALYZE=5    # Max analyze requests per minute
RATE_LIMIT_MARKETS=30   # Max market requests per minute

# Optional: Cache TTL in seconds (default: 60)
CACHE_TTL=60
